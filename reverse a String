public class Solution {
    public String reverseString(String s) {
        char a[]=s.toCharArray();
        int i=0;
        int l=s.length()-1;
        while(i<l)
        {
            char t=a[i];
            a[i]=a[l];
            a[l]=t;
            i++;
            l--;
        }
        String x=new String(a);
        return x;
    }
}
